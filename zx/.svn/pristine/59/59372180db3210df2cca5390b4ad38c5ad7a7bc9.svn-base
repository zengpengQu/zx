package com.taiji.frame.common.util;

/**
 * 导入 Excel 错误码
 * @author yx
 */
public enum ImportExcelEnum {
    // 导入成功
    SUCCESS(0),

    // 空 Excel
    ERROR_EMPTY_EXCEL(1),
    // Excel 标题错误
    ERROR_WRONG_TITLE(2),
    // 单位编码不正确
    ERROR_WRONG_DEPT_NUM(3),
    // 日期格式不正确
    ERROR_WRONG_DATE(4),
    // 电话号码格式不正确
    ERROR_WRONG_PHONE(5),
    // 板块编码错误
    ERROR_WRONG_SECTION(6),

    // 保存数据库失败
    ERROR_ON_SAVE(7);

    private final int value;
    ImportExcelEnum(int value) {
        this.value = value;
    }
}

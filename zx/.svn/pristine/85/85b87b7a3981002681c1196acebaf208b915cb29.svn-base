.boxtop{background:url(../../images/index/qxj_topbgimg.jpg) no-repeat center top;width:100%;}
.logininfo{text-align:right;overflow:hidden;line-height:25px;color:#fefcca;margin-top:10px;font-size:12px;}
.logininfo a{color:#fefcca;}
.menubg{background:url(../../images/index/qxj_menuarrow.png) no-repeat center top;height:43px;}
.menubg ul{float:right;max-width: 970px;}
.menubg li{padding:0px 23px;background:url(../../images/index/menubgline.png) no-repeat right top;float:left;height:43px;line-height:43px;position:relative;cursor:pointer;font-weight: bold;}
.menubg li:hover{height:41px;border-bottom:2px solid #2b6dbc;}
.menubg li:hover .sondiv{display:block;
    z-index: 999;}
.sondiv{display:none;background-color:#fff; background-image:url(../../images/index/qxj_arrow.png);background-repeat:no-repeat;background-position:center -3px;width:100%;overflow:hidden;position:absolute;top:41px;left:0px;z-index:5;padding:15px 0px;}
.sondiv dl{line-height:28px;border-bottom:1px dashed #F4FAFB;font-size:12px;color:#42748C;margin:0px 15px;}
.sondiv dl a{color:#42748C;}
.sondiv dl a:hover{color:#CE721E;text-decoration:none;cursor:pointer;}
.positioninfo{float:left;line-height:40px;min-width:375px;font-size:14px;color:#951616;font-weight:bold;padding-left:35px;}
.positioninfoTab{float:left;line-height:40px;width:450px;font-size:14px;color:#951616;font-weight:bold;padding-left:35px;}
.positionimg{position:absolute;left:-5px;top:5px;z-index:10;}
.scrollnews{float:right;font-size:12px;color:#951616;width:640px;padding-top:10px;overflow:hidden;}
.scrollnews li{float:left;color:#951616;}
.scrollnews li a{margin:0px 15px;color:#951616;}
.block{background:#fff;overflow:hidden;}
.block .th{padding:0px 20px 0px 15px;height:39px;color:#104585;font-weight:bold;font-size:15px;line-height:39px;
    /*background:url(../../images/index/qxj_ico.png) no-repeat 20px center;*/
}

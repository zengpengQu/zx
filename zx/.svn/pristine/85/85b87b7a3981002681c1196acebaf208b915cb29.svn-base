.line3_dreepblue{height:2px;background:#99b6d7;}
.line3_blue{height:2px;background:#bfddfe;width:120px;}
.bigimgblock{width:327px;float:left;}
.bigimgblock li{overflow:hidden;line-height:25px;color:#555;font-size:12px;font-family:"宋体"}
.bigimgblock li .div1{width:240px;white-space:nowrap;text-overflow:ellipsis;overflow: hidden;float:left;}
.bigimgblock li .div2{width:70px;color:#898989;float:right;text-align:right;}
.titlebg{background:url(../../images/index/bgimg_titbg.jpg) no-repeat;height:35px;}
.titlebg .font{width:200px;white-space:nowrap;text-overflow:ellipsis;overflow: hidden;font-size:16px;color:#555;line-height:35px;float:left;padding-left:10px;}
.titlebg .img{float:right;width:75px;}
.titlebg .img img{margin:13px 4px 0px 0px;}
.newsblock{width:282px;float:right;}
.newsblock li{padding-bottom:8px;margin-bottom:9px;border-bottom:1px dashed #d5d5d5;font-size:12px;overflow:hidden;line-height:23px;}
.newslist{width:225px;float:right;}
.newslist dt .font1{display:block;float:left;width:150px;white-space:nowrap;text-overflow:ellipsis;overflow: hidden;font-size:14px;color:#2865ac;}
.newslist dt .font1 a{color:#2865ac;}
.newslist dt .font2{display:block;float:right;width:70px;font-size:12px;color:#c9c9c9;text-align:right;}
.newslist dt .font2 a{color:#c9c9c9;}
.newslist dt .font2 img{margin-top:5px;}
.newslist dt .font2 div{float:right;color:#c9c9c9;}
.newslist dd{overflow:hidden;width:200px;white-space:nowrap;text-overflow:ellipsis;}
.gg_list{font-size:13px;margin-top:-7px;}
.gg_list li{border-bottom:1px dashed #d5d5d5;overflow:hidden;line-height:30px;}
.gg_list li .datediv{float:right;font-size:12px;color: #898989;width:120px;text-align:right;}
.gg_list li .fontdiv{float:right;font-size:13px;color:#555;width:128px;white-space:nowrap;text-overflow:ellipsis;overflow: hidden;}
.gg_list li .fontdiv a{color:#555;}
.gg_list li .icodiv{float:left;}
